# OpenAI API 설정
# OpenAI 웹사이트에서 API 키를 발급받아 입력하세요
# https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# API 사용 여부 (true/false)
USE_AI_JUDGE=true

# Riot API 설정 (전적 검색 기능용)
# https://developer.riotgames.com/ 에서 발급
RIOT_API_KEY=your_riot_api_key_here

# Google AdSense 설정
# https://www.google.com/adsense 에서 발급
GOOGLE_ADSENSE_CLIENT_ID=ca-pub-XXXXXXXXXXXXXXXX

# Google Analytics 설정 (선택사항)
# https://analytics.google.com/ 에서 발급
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# 광고 표시 여부 (true/false)
SHOW_ADS=true
